#

JAVA_OPTS="\
-Dcom.sun.jini.jeri.connectionTimeout=2400000 \
-Dlia.Monitor.isVRVS=true \
-DMonaLisa.Location=${EVO_INST} \
-DMonaLisa.Country=${EVO_COUNTRY} \
-DMonaLisa.LAT=${EVO_LAT} \
-DMonaLisa.LONG=${EVO_LON} \
${JAVA_OPTS}"
